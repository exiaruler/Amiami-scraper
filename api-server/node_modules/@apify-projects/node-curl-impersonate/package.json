{
    "name": "@apify-projects/node-curl-impersonate",
    "version": "1.0.0",
    "description": "A wrapper around cURL-impersonate, a binary which can be used to bypass TLS fingerprinting.",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "bin"
    ],
    "devDependencies": {
        "@apify/eslint-config-ts": "^0.4.1",
        "@apify/tsconfig": "^0.1.0",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.5.9",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "eslint": "^8.57.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.7",
        "tslib": "^2.6.3",
        "typescript": "^5.5.3",
        "vitest": "^1.6.0"
    },
    "scripts": {
        "build": "tsc",
        "lint": "eslint ./src --ext .ts",
